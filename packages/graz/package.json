{
  "name": "graz",
  "description": "React hooks for Cosmos",
  "version": "0.0.18",
  "author": "Griko Nibras <griko@strangelove.ventures>",
  "repository": "https://github.com/strangelove-ventures/graz.git",
  "homepage": "https://github.com/strangelove-ventures/graz",
  "bugs": "https://github.com/strangelove-ventures/graz/issues",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./chains": {
      "module": "./chains/index.mjs",
      "import": "./chains/index.mjs",
      "default": "./chains/index.js"
    }
  },
  "bin": {
    "graz": "./cli.mjs"
  },
  "files": [
    "chains/*.stub",
    "cli.js",
    "compiled/**",
    "dist/*.d.ts",
    "dist/*.js",
    "dist/*.mjs"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "cli": "node cli.mjs",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.28.11",
    "@cosmjs/proto-signing": "^0.28.11",
    "@cosmjs/stargate": "^0.28.11",
    "@keplr-wallet/cosmos": "^0.10.13",
    "@keplr-wallet/types": "^0.10.13",
    "@tanstack/react-query": "^4.0.10",
    "arg": "^5",
    "cosmos-directory-client": "0.0.6",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^16",
    "@types/react": "^18",
    "react": "^18",
    "typescript": "^4"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "keywords": [
    "graz",
    "keplr-wallet",
    "keplr",
    "react-cosmos-hooks",
    "strangelove-ventures",
    "use-keplr"
  ],
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "license": "MIT"
}
